<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.envcheck.base.bean.discharge_check_history.CheckPeriodRecord" table="check_period_record">

		<id name="id" type="java.lang.String">
			<column name="id" length="32" />
			<generator class="uuid"></generator>
		</id>

		<property name="periodEndTime" type="java.util.Date" >
			<column name="period_end_time"></column>
		</property>

		<property name="periodName" type="java.lang.String" >
			<column name="period_name"></column>
		</property>

	</class>

</hibernate-mapping>

