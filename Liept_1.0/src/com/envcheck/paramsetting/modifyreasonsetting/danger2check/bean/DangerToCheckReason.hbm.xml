<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.envcheck.paramsetting.modifyreasonsetting.danger2check.bean.DangerToCheckReason" table="danger_to_check_reason">

		<id name="id" type="java.lang.String">
			<column name="id" length="32" />
			<generator class="uuid"></generator>
		</id>


		<property name="dangerToCheckReasonNumber" column="danger_to_check_reason_number" type="java.lang.String" length="40"  ></property>

		<property name="dangerToCheckReasonName"  column="danger_to_check_reason_name" type="java.lang.String" length="20" ></property>

		<property name="createTime"  column="create_time" type="java.util.Date" length="20" ></property>
		<property name="createPerson" column="create_person" type="java.lang.String"  length="20" ></property>
		<property name="modifyTime" column="modify_time" type="java.util.Date"  length="20" ></property>
		<property name="modifyPerson" column="modify_person" type="java.lang.String" length="20" ></property>


		
	</class>

</hibernate-mapping>

