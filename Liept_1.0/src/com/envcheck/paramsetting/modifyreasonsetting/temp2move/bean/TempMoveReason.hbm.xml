<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.envcheck.paramsetting.modifyreasonsetting.temp2move.bean.TempMoveReason" table="temp_move_reason">

		<id name="id" type="java.lang.String">
			<column name="id" length="32" />
			<generator class="uuid"></generator>
		</id>


		<property name="tempMoveReasonName" type="java.lang.String" >
			<column name="temp_move_reason_name" length="20" not-null="true"></column>
		</property>

		<property name="tempMoveReasonNumber" type="java.lang.String" >
			<column name="temp_move_reason_number" length="40" not-null="true"></column>
		</property>


		<property name="createTime"  column="create_time" type="java.util.Date"  length="20"></property>
		<property name="createPerson" column="create_person" type="java.lang.String"  length="20"></property>
		<property name="modifyTime" column="modify_time" type="java.util.Date"  length="20"></property>
		<property name="modifyPerson" column="modify_person" type="java.lang.String"  length="20"></property>


		
	</class>

</hibernate-mapping>

