<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.envcheck.base.bean.ModuleRepairUnit" table="module_repair_unit">


		<id name="id" type="java.lang.String">
			<column name="id" length="64" />
		</id>

		<property name="repairDate" type="java.util.Date" >
			<column name="repair_date"></column>
		</property>
		<property name="repairPerson" type="java.lang.String" >
			<column name="repair_person"></column>
		</property>
		<property name="repairMeasure" type="java.lang.String" >
			<column name="repair_measure"></column>
		</property>
		<property name="repeateCheckDate" type="java.util.Date" >
			<column name="repeate_check_date"></column>
		</property>
		<property name="repeateCheckValue" type="java.lang.Integer" >
			<column name="repeate_check_value"></column>
		</property>

	</class>

</hibernate-mapping>

