<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="importmodule" namespace="/tool" extends="basePackge">

		<action name="import_module_*" class="com.envcheck.tool.importmodule.action.ImportModuleAction" method="{1}">
			<result name="{1}">/WEB-INF/jsp/tool/importmodule/{1}.jsp</result>
			<result name="list" type="redirectAction">
				<param name="actionName">import_module_listUI</param>
			</result>
		</action>
	</package>

</struts>
