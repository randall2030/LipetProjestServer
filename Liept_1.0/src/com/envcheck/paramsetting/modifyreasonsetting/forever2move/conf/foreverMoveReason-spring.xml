<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd

	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">


    <bean id="foreverMoveReasonDao" class="com.envcheck.paramsetting.modifyreasonsetting.forever2move.dao.impl.ForeverMoveReasonDao">
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="foreverMoveReasonService" class="com.envcheck.paramsetting.modifyreasonsetting.forever2move.service.impl.ForeverMoveReasonService">
        <property name="foreverMoveReasonDao" ref="foreverMoveReasonDao"></property>
    </bean>

    <bean id="foreverMoveReasonAction" class="com.envcheck.paramsetting.modifyreasonsetting.forever2move.action.ForeverMoveReasonAction">
        <property name="foreverMoveReasonService" ref="foreverMoveReasonService"></property>
    </bean>

</beans>

