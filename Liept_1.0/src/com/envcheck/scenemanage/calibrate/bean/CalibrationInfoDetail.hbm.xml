<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.envcheck.scenemanage.calibrate.bean.CalibrationInfoDetail" table="calibrate_detail">

		<id name="id" type="java.lang.String">
			<column name="id" length="100" />
		</id>


		<property name="standGasNumber" type="java.lang.String" >
			<column name="stand_gas_number" length="30" ></column>
		</property>

		<property name="standGasReadNumber" type="java.lang.String" >
			<column name="stand_gas_read_number" length="30" ></column>
		</property>

		<property name="caculateNumber" type="java.lang.String" >
			<column name="caculate_number" length="30" ></column>
		</property>

		<property name="calibrationReadNumber" type="java.lang.String" >
			<column name="calibration_read_number" length="30" ></column>
		</property>

		<property name="responseTime" type="java.lang.String" >
			<column name="response_time" length="30" ></column>
		</property>

		<property name="ppmThresholdNumber" type="java.lang.String" >
			<column name="ppm_threshold_number" length="30" ></column>
		</property>

		<property name="createTime" type="java.util.Date" >
			<column name="create_time" length="30" ></column>
		</property>

		<property name="calibrationResult" type="java.lang.Boolean" >
			<column name="calibration_result" length="30" ></column>
		</property>


	</class>

</hibernate-mapping>

